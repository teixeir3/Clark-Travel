<div class="col-xs-6 col-xs-offset-3 row-padding">
  <div class="form text-center">
    <form class="form" action="<%= session_url %>" method="post">
      <%= auth_token %>

      <div class="page-header text-center">
        <h1>Welcome Agent</h1>
      </div>
    
      <div class="form-group">
        <label for="email">Email:</label>
        <input type="text" class="form-control" name="user[email]" value="" id="email">
      </div>

      <div class="form-group">
        <label for="password">Password:</label>
        <input type="password" class="form-control" id="password" name="user[password]" value="">
      </div>

      <div class="form-group submit">
        <input type="submit" value="Log in" class="btn">
      </div>
    
      <div class="google-signin">
        <a class="group" href="/auth/google_oauth2"></a>
      </div>
    </form>   
  </div>
</div>
